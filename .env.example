# Supabase Configuration
VITE_SUPABASE_PROJECT_ID="your_project_id"
VITE_SUPABASE_PUBLISHABLE_KEY="your_publishable_key"
VITE_SUPABASE_URL="https://your_project_id.supabase.co"

# Google Maps API
# IMPORTANT: Restreindre cette clé aux domaines autorisés dans Google Cloud Console
VITE_GOOGLE_MAPS_API_KEY="your_google_maps_api_key"

# Firebase Configuration
# Récupérer depuis Firebase Console > Project Settings > General
VITE_FIREBASE_API_KEY="your_firebase_api_key"
VITE_FIREBASE_AUTH_DOMAIN="your_project.firebaseapp.com"
VITE_FIREBASE_PROJECT_ID="your_firebase_project_id"
VITE_FIREBASE_STORAGE_BUCKET="your_project.appspot.com"
VITE_FIREBASE_MESSAGING_SENDER_ID="your_sender_id"
VITE_FIREBASE_APP_ID="your_app_id"

# Firebase VAPID Key (Web Push Certificates)
# Récupérer depuis Firebase Console > Project Settings > Cloud Messaging
VITE_FIREBASE_VAPID_KEY="your_vapid_public_key"
